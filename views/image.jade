.container

  .row
    .span16.columns
      h1= share.data.caption
      a(href="#{ share.getImgUrl() }")
        img(src="#{ share.getSmallImgUrl() }", alt="")

      p by #{ share.data.nick } @ #{ share.data.network }
      p Page views: #{ share.data.views }
      p Status: #{ share.data.status }
